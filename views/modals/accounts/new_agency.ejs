<div class="modal fade" id="modal-new-agency" role="dialog" aria-labelledby="modal-default-popout" aria-hidden="true"
	data-keyboard="false" data-bs-backdrop="static">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">



			<div class="modal-header bg-primary">
			<h5 class="block-title text-white"><%= __('modal_new_agency.new_agency') %></h5>
			<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
			</div>

			
			<div class="modal-body">
			<form action="/add_agency" method="post">
				<div class="block-content">
					<div class="row mb-2">
						<div class="col-sm-12">
							<input type="text" class="form-control" name="txtAgency" placeholder="<%= __('modal_new_agency.agency_name_placeholder') %>">
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="submit" class="btn btn-sm btn-primary" id="submit-new-agency-btn" data-label="<%= __('modal_new_agency.save') %>">
						<%= __('modal_new_agency.save') %>
					</button>
					
					<button type="button" class="btn btn-sm btn-alt-secondary" data-bs-dismiss="modal"><%= __('modal_new_agency.close') %></button>
				</div>
			</form>
			</div>
		</div>
	</div>
</div>
